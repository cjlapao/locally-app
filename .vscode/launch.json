    "version": "0.2.0",
    "configurations": [
        {
            "name": "locally.app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file",
                "api-keys.dev.json"
            ]
        },
        {
            "name": "locally.app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file",
                "api-keys.dev.json"
            ]
        },
        {
            "name": "locally.app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file",
                "api-keys.prd.json"
            ]

        }
    ]
}